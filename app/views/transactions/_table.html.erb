<div class="tcm-table"
     data-path="<%= regime_transactions_path(view_model.regime) %>"
     data-summary-path="<%= regime_transaction_summary_index_path(view_model.regime) %>"
     data-approve-path="<%= approve_regime_transactions_path(view_model.regime, format: :json) %>"
     data-region="<%= view_model.region %>"
     data-financial-year="<%= view_model.financial_year %>"
     data-unapproved="<%= view_model.unapproved %>"
     data-search="<%= view_model.search %>"
     data-page="<%= view_model.page %>"
     data-per-page="<%= view_model.per_page %>"
     data-sort-column="<%= view_model.sort %>"
     data-sort-direction="<%= view_model.sort_direction %>">
<%= render partial: 'transaction_search_bar', locals: { view_model: view_model, view_mode: 'unbilled' } %>
<%= render partial: view_model.table_partial_name,
  locals: { view_model: view_model } %>
<%= render partial: "shared/pagination", locals: { collection: view_model.present_paged_transactions, include_export: current_user.can_export_data? } %>
  <div id="summary-dialog"
       data-summary-path="<%= regime_transaction_summary_index_path(view_model.regime) %>">
  </div>
  <%= render partial: "shared/data_protection_dialog" %>
</div>
